{
	"name": "rest_api_store_postgre_express",
	"version": "1.0.0",
	"description": "",
	"scripts": {
		"start": " db-migrate up && npm run build && node ./dist/server.js",
		"clean": "rimraf ./dist",
		"build": "npm run clean && npx tsc",
		"watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
		"prettier-format": "prettier --config .prettierrc src/**/*.ts --write",
		"lint": "eslint . --ext .ts --quiet",
		"lint-fix": "eslint . --ext .ts --fix",
		"migrate-test": "cross-env ENV=test db-migrate up",
		"jasmine": "ENV=test jasmine-ts --config=jasmine.json",
		"test-db-create": " npm run test-db-down && db-migrate db:create test_db --config ./database_create.json",
		"test-db-up": "db-migrate up --config ./database_migration.json",
		"test-db-down": "db-migrate --config ./database_create.json db:drop test_db",
		"test": "npm run test-db-create && npm run test-db-up && npm run jasmine && npm run test-db-down"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"body-parser": "^1.20.0",
		"dotenv": "^16.0.0",
		"express": "^4.17.3",
		"express-validator": "^6.14.0",
		"pg": "^8.7.3"
	},
	"devDependencies": {
		"@types/dotenv": "^8.2.0",
		"@types/express": "^4.17.13",
		"@types/jasmine": "^4.0.3",
		"@types/node": "latest",
		"@types/pg": "^8.6.5",
		"@types/supertest": "^2.0.12",
		"@types/validator": "^13.7.2",
		"cross-env": "^7.0.3",
		"db-migrate": "^0.11.13",
		"db-migrate-pg": "^1.2.2",
		"eslint": "^8.13.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.0.0",
		"jasmine": "^3.99.0",
		"jasmine-spec-reporter": "^7.0.0",
		"jasmine-ts": "^0.4.0",
		"prettier": "^2.6.2",
		"rimraf": "^3.0.2",
		"supertest": "^6.2.3",
		"ts-node": "^10.7.0",
		"tsc-watch": "^5.0.3",
		"typescript": "^4.6.3"
	}
}
